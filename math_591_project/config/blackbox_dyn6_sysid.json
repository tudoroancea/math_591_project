{
    "model": {
        "name": "blackbox_dyn6",
        "n_hidden": [
            128,
            128,
            128
        ],
        "nonlinearity": "leaky_relu",
        "from_checkpoint": false
    },
    "training": {
        "num_epochs": 500,
        "Nf": 10,
        "batch_size": 30000,
        "loss_weights": {
            "XY": 1.0,
            "phi": 10.0,
            "v_x": 1.0,
            "v_y": 1.0,
            "r": 1.0
        },
        "optimizer": {
            "name": "adamw",
            "lr": 0.0001,
            "weight_decay": 0.05
        },
        "scheduler": {
            "name": "multisteplr",
            "milestones": [
                1000
            ],
            "gamma": 0.2
        }
    },
    "testing": {
        "num_samples": 10,
        "Nf": 40
    },
    "data": {
        "dir": "data_po",
        "train": "train",
        "test": "test"
    },
    "with_wandb": true
}