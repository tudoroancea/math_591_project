{
    "model": {
        "name": "blackbox_dyn6",
        "n_hidden": [
            128,
            128,
            128
        ],
        "nonlinearity": "leaky_relu",
        "from_checkpoint": false
    },
    "training": {
        "num_epochs": 2000,
        "loss_weights": {
            "XY": 10.0,
            "phi": 100.0,
            "v_x": 50.0,
            "v_y": 100.0,
            "r": 1.0
        },
        "optimizer": {
            "name": "adamw",
            "lr": 0.0005,
            "weight_decay": 0.05
        },
        "scheduler": {
            "name": "multisteplr",
            "milestones": [
                1000
            ],
            "gamma": 0.2
        }
    },
    "testing": {
        "num_samples": 10
    },
    "data": {
        "train": "train",
        "test": "test"
    },
    "with_wandb": true
}